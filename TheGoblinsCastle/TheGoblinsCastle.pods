    <Name>Замъкът на таласъмите</Name>
    <InitialEpisode>342</InitialEpisode>
    <Description>Тази книга не ще ти предложи да следиш чужди приключения. Героят в нея си самият ти. Единствено от теб, от твоята изобретателност и предвидливост, от твоята храброст и решителност зависи дали ще изпълниш поставената задача, или ще загинеш в средата на мисията. Не вярваш ли? Чети тогава и не пропускай правилата на приключението. От теб се очаква да проникнеш в замъка на таласъмите и да победиш страшния Тиквеняк Първи и неговата охрана от таласъми и чукундурти.</Description>
    <Discoverable>True</Discoverable>
  </Properties>
  <GlobalScripts>
    <Script Name="items">context.addItem = function(itemId, stack) {
  if (!stack) {
    stack = 1;
  }
  if (this.getWeight() &lt; this.player.carryLimit) {
    this.player.addItem(this.createItem(itemId, stack));
    this.showPopupMessage(`Имаш ${this.player.getItem(itemId).stack}`);
  } else {
    this.showPopupMessage('Не можеш да носиш повече!');
  }
  return false;
}

context.removeItem = function(itemId, stack) {
  if (!stack) {
    stack = 1;
  }
  this.player.removeItem(itemId, stack);
  const item = this.player.getItem(itemId);
  if (item) {
    this.showPopupMessage(`Остават ${item.stack}`);
  } else {
    this.showPopupMessage('Остават 0');
  }
  return false;
}

context.getWeight = function() {
  let weight = 0;
  Object.keys(this.player.items).forEach(k =&gt; weight += this.player.items[k].stack * this.player.items[k].weight);
  return weight;
}

context.getSelectedItemMessage = function(selectedItems, itemsToThrow) {
  let result = "Избрал си: ";
  for (const [key, value] of Object.entries(selectedItems)) {
    if (value === 0) {
      continue;
    }
    const item = this.getItem(key);
    result += `${item.name} x${value}, `;
  }
  result += `Остават: ${itemsToThrow}`;
  return result;
}

context.getItemCount = function(items) {
  let result = 0;
  for (const [key, value] of Object.entries(items)) {
    if (key === 'gold') {
      result += value / 10;
    } else {
      result += value;
    }
  }
  return result;
}</Script>
    <Script Name="player">context.increasePlayerLife = function(value) {
  this.player.life = Math.min(this.player.life + value, this.player.maxLife);
}

context.reducePlayerLife = function(value) {
  this.player.life -= value;
  if (this.player.life &lt;= 0) {
    context.gameOver();
    return context.DEATH_MESSAGE;
  }
}</Script>
