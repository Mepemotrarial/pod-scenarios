  <GlobalScripts>
    <Script Name="player">context.increasePlayerEndurance = function(value) {
  this.player.endurance = Math.min(this.player.endurance + value, this.player.maxEndurance);
}

context.reducePlayerEndurance = function(value, skipDeathCheck) {
  this.player.endurance -= value;
  if (this.player.endurance &lt;= 0 &amp;&amp; !skipDeathCheck) {
    context.gameOver();
    return context.DEATH_MESSAGE;
  }
}

context.increasePlayerBattleSkill = function(value) {
  this.player.battleSkill += value;
  this.updatePlayerBattleSkill();
}

context.reducePlayerBattleSkill = function(value) {
  this.player.battleSkill -= value;
  this.updatePlayerBattleSkill();
}

context.updatePlayerBattleSkill = function() {
  this.player.attack = 3 + Math.floor(this.player.battleSkill / 20);
  this.player.maxEndurance = 15 + Math.floor(this.player.battleSkill / 5);
  if (this.player.endurance &gt; this.player.maxEndurance) {
    this.player.endurance = this.player.maxEndurance;
  }
}</Script>
    <Script Name="items">context.hasMoney = function(kronas, kamurs) {
  const playerMoney = this.getPlayerMoney();
  return kronas * 20 + kamurs &lt;= playerMoney.kronas * 20 + playerMoney.kamurs;
}

context.removeMoney = function(type, amount) {
  let kamurs = this.player.getItem('kamur');
  if (kamurs) {
    kamurs = kamurs.stack;
  } else {
    kamurs = 0;
  }
  let kronas = this.player.getItem('krona');
  if (kronas) {
    kronas = kronas.stack;
  } else {
    kronas = 0;
  }

  if (type === 'kamur') {
    while (kamurs &lt; amount) {
      if (kronas &gt; 0) {
        kronas -= 1;
        kamurs += 20;
      } else {
        // Not enough
        return false;
      }
    }
    kamurs -= amount;
  }
  if (type === 'krona') {
    while (kronas &lt; amount) {
      if (kamurs &gt;= 20) {
        kamurs -= 20;
        kronas += 1;
      } else {
        // Not enough
        return false;
      }
    }
    kronas -= amount;
  }
  this.removeItem(this.player, 'kamur');
  this.removeItem(this.player, 'krona');
  if (kamurs &gt; 0) {
    this.addItem(this.player, 'kamur', kamurs);
  }
  if (kronas &gt; 0) {
    this.addItem(this.player, 'krona', kronas);
  }
  return true;
}

context.getPlayerMoney = function() {
  let kamurs = this.player.getItem('kamur');
  if (kamurs) {
    kamurs = kamurs.stack;
  } else {
    kamurs = 0;
  }
  let kronas = this.player.getItem('krona');
  if (kronas) {
    kronas = kronas.stack;
  } else {
    kronas = 0;
  }
  return { kronas: kronas, kamurs: kamurs };
}

context.getItemPrice = function(items) {
  price = { kronas: 0, kamurs: 0 };
  if (!items) {
    return price;
  }

  for (let i = 0; i &lt; items.length; i++) {
    price.kronas += items[i].priceKrona;
    price.kamurs += items[i].priceKamur;
  }
  return price;
}

context.simplifyPrice = function(price) {
  if (!price) {
    return price;
  }

  price.kronas += Math.floor(price.kamurs / 20);
  price.kamurs = price.kamurs % 20;
  return price;
}

context.isPriceGreaterThan = function(price, money) {
  // Convert to kamurs for simplicity
  return price.kronas * 20 + price.kamurs &gt; money.kronas * 20 + money.kamurs;
}</Script>
